<h1> Mitarbeiter editieren</h1>
<%= form_for(@employee) do |f| %>
    <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name %>
        <%= f.label :email %>
        <%= f.text_field :email %>
        <%= f.submit :Submit, class: "btn btn-primary" %>
    </div>
<% end %>